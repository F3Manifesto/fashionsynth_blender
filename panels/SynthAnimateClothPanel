import bpy

class SynthAnimateClothPanel(bpy.types.Panel):
    bl_label="Animate Cloth Panel"
    bl_idname="SYNTH_PT_Animate"
    bl_space_type="VIEW_3D"
    bl_region_type="UI"
    bl_category="Fashion Synth"
    
    def draw(self, context):
        layout = self.layout
        
        row = layout.row()
        row.label(text="Animate Cloth")
        
